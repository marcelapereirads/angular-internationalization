{
  "name": "angular-internationalization",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ng xi18n angular-internationalization --i18n-format xlf --output-path i18n --i18n-locale pt && ng run angular-internationalization:xliffmerge",
